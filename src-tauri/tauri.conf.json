{
  "build": {
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build:web",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "网易云音乐云盘上传",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "http": {
        "all": true,
        "request": true,
        "scope": ["https://music.163.com/*", "https://interface.music.163.com/*"]
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.ovnrain.netease-music-cloud-uploader",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "我的音乐云盘",
        "width": 1000,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ]
  }
}
